{
  "name": "portfolio-jm",
  "version": "1.0.0",
  "description": "Load libraries for working with Jacques Mathieu's Portfolio Website",
  "main": "index.js",
  "dependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-plugin-inline-json-import": "^0.3.2",
    "bytenode": "^1.1.6",
    "dot-prop": "^5.2.0",
    "nodemon": "^2.0.14",
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/runtime": "^7.11.0",
    "concat-files": "^0.1.1",
    "del-cli": "^3.0.1",
    "jsdom": "15.1.1",
    "jsdom-global": "3.0.2",
    "mocha": "^8.4.0",
    "react-dom": "^16.13.1"
  },
  "nodemonConfig": {
    "ignore": [
      "tests",
      "img",
      "js",
      "concat"
    ],
    "events": {
      "restart": "npm run build"
    }
  },
  "scripts": {
    "build": "npm run clearJSProdDir && babel --presets @babel/preset-react js/pre-prod -d js/prod && npm run readyProd",
    "clearJSProdDir": "del-cli js/prod",
    "readyProd": "node concat/scripts/concat-all-components.js",
    "start": "npm run build && nodemon --exec node js/prod/server/init.js",
    "buildTests": "babel --presets @babel/preset-env tests/pre -d tests/build && babel --plugins @babel/plugin-transform-runtime tests/build -d tests/build",
    "test": "npm run buildTests && npm run execConcats && npm run _mocha",
    "_mocha": "mocha tests/build -r jsdom-global/register",
    "execConcats": "node concat/scripts/utility-nodify.js && node concat/scripts/function-components-nodify.js && node concat/scripts/section-link-nodify.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jmathieu14/Portfolio.git"
  },
  "author": "Jacques Mathieu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jmathieu14/Portfolio/issues"
  },
  "homepage": "https://github.com/Jmathieu14/Portfolio#readme"
}
